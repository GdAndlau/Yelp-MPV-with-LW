<%= render "navbar", restaurants: @restaurants %>
<h1>Would you grace us with another restaurant?</h1>

<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %><br />
  <%= f.input :address %><br />
  <%= f.input :phone_number %><br />
  <%= f.input :category %><br />
  <%= f.submit %>
<% end %>

